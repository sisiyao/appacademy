<h2>Log in for MusicApp</h2>

<p>
  Don't have an account? <%= link_to "Create an account here>>", new_user_url %>
</p>

<form action="/session" method="post">
  <%= auth_token %>
  <label>Email:
    <input type="email" name="user[email]">
  </label>

  <label>Password:
    <input type="password" name="user[password]">
  </label>

  <input type="submit" value="Sign in">
</form>
